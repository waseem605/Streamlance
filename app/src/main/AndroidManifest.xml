<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.streamlance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Streamlance">
        <activity android:name=".Login.Login"></activity>
        <activity android:name=".Login.Password" />
        <activity android:name=".Login.RegisterUser" />
        <activity android:name=".Login.VerifyOtp" />
        <activity android:name=".common.OTP_Screen" />
        <activity android:name=".common.Onboarding" />
        <activity android:name=".common.Splash_Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>